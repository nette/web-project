parameters:
	ignoreErrors:
		-
			message: '#^Class Nette\\Assets\\AudioAsset has an uninitialized readonly property \$duration\. Assign it in the constructor\.$#'
			identifier: property.uninitializedReadonly
			count: 1
			path: src/Assets/AudioAsset.php

		-
			message: '#^Class Nette\\Assets\\GenericAsset has an uninitialized readonly property \$mimeType\. Assign it in the constructor\.$#'
			identifier: property.uninitializedReadonly
			count: 1
			path: src/Assets/GenericAsset.php

		-
			message: '#^Readonly property Nette\\Assets\\ImageAsset\:\:\$height is already assigned\.$#'
			identifier: assign.readOnlyProperty
			count: 1
			path: src/Assets/ImageAsset.php

		-
			message: '#^Readonly property Nette\\Assets\\ImageAsset\:\:\$height is assigned outside of the constructor\.$#'
			identifier: property.readOnlyAssignNotInConstructor
			count: 1
			path: src/Assets/ImageAsset.php

		-
			message: '#^Readonly property Nette\\Assets\\ImageAsset\:\:\$width is already assigned\.$#'
			identifier: assign.readOnlyProperty
			count: 1
			path: src/Assets/ImageAsset.php

		-
			message: '#^Readonly property Nette\\Assets\\ImageAsset\:\:\$width is assigned outside of the constructor\.$#'
			identifier: property.readOnlyAssignNotInConstructor
			count: 1
			path: src/Assets/ImageAsset.php

		-
			message: '#^Call to an undefined method Nette\\DI\\Definitions\\Definition\:\:getResultDefinition\(\)\.$#'
			identifier: method.notFound
			count: 1
			path: src/Bridges/AssetsDI/DIExtension.php

		-
			message: '#^Cannot access property \$baseUrl on array\|object\.$#'
			identifier: property.nonObject
			count: 2
			path: src/Bridges/AssetsDI/DIExtension.php

		-
			message: '#^Unsafe usage of new static\(\)\.$#'
			identifier: new.static
			count: 1
			path: src/Bridges/AssetsLatte/Nodes/AssetNode.php
