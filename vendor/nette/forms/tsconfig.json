{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2022",
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"strict": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true
	},
	"include": [
		"src/assets/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
